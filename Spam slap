-- CENTRAL LOADER - prozin666
-- Executa m√∫ltiplos scripts automaticamente

print("[Central Loader] Iniciando...")

local scripts = {
    -- Spam Slap
    "https://raw.githubusercontent.com/prozin666/Spam-slap-ez-trol/main/Slap",

    -- Anti Slap
    "https://raw.githubusercontent.com/prozin666/Slap-tower-1-atualizado/main/Anti%20slap",

    -- Sentar com fixador
    "https://raw.githubusercontent.com/prozin666/Script-de-sentar-com-fixador/main/Sentar%20com%20fixador",

    -- Excluir porta de TP / Aura EZ Troll
    "https://raw.githubusercontent.com/prozin666/Excluir-portaa-de-tp-ez-troll-/refs/heads/main/Ez%20troll%20muita%20aura",
}

for _, url in ipairs(scripts) do
    task.spawn(function()
        local ok, err = pcall(function()
            loadstring(game:HttpGet(url))()
        end)

        if ok then
            print("[Central Loader] Executado com sucesso:", url)
        else
            warn("[Central Loader] Erro ao executar:", url)
            warn(err)
        end
    end)
end

print("[Central Loader] Todos os scripts foram disparados.")
